{
    "name": "botman/botman",
    "license": "MIT",
    "description": "Create messaging bots in PHP with ease.",
    "keywords": [
        "Bot",
        "BotMan",
        "Chatbot"
    ],
    "homepage": "http://github.com/dostrog/botman",
    "authors": [
        {
            "name": "Marcel Pociot",
            "email": "m.pociot@gmail.com"
        },
        {
            "name": "Sergey Donin",
            "email": "sergey.donin@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "opis/closure": "^2.3 || ^3.0",
        "mpociot/pipeline": "^1.0",
        "react/socket": "~1.0",
        "spatie/macroable": "^1.0",
        "psr/container": "^1.0"
    },
    "require-dev": {
        "codeigniter/framework": "~3.0",
        "mockery/mockery": "^1.1",
        "doctrine/cache": "^1.6",
        "symfony/cache": "^3.1",
        "ext-curl": "*",
        "orchestra/testbench": "^4.0"
    },
    "suggest": {
        "doctrine/cache": "Use any Doctrine cache driver for cache",
        "symfony/cache": "Use any Symfony cache driver for cache"
    },
    "autoload": {
        "psr-4": {
            "BotMan\\BotMan\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BotMan\\BotMan\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.1-dev"
        },
        "laravel": {
            "providers": [
                "BotMan\\BotMan\\BotManServiceProvider"
            ],
            "aliases": {
                "BotMan": "BotMan\\BotMan\\Facades\\BotMan"
            }
        }
    },
    "scripts": {
        "test": "phpunit",
        "cs": "php-cs-fixer fix"
    }
}
